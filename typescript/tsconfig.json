{
	"compilerOptions": {
		"target": "es5",
		"inlineSourceMap": true,
		"sourceRoot": "typescript",
		"downlevelIteration": true,
		"experimentalDecorators": true,
		"lib": [
			"dom",
			"es2016"
		],
		"jsx": "react",
		"outFile": "build.js"
	},
	"include": [
		"typings/**/*.ts",
		// Make sure main.ts is put LAST
		"main.tsx",
	]
}
